        "@types/jsdom": "^21.1.6",
        "@types/jsonwebtoken": "^9.0.6",
        "@typescript-eslint/eslint-plugin": "^7.8.0",
        "@typescript-eslint/parser": "^7.8.0",
        "autoprefixer": "^10.4.19",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.38.0",
